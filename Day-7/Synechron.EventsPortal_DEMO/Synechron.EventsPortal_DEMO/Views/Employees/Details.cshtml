@model Synechron.EventsPortal_DEMO.Models.Employee

@{
    ViewBag.Title = "Employee Details";
}



<h2>Details of - @Model.EmployeeName  </h2>

<table class="table table-hover table-striped">
    <tbody>
        <tr>
            <th>Employee Id</th>
            <td>@Model.EmployeeId</td>
        </tr>
        <tr>
            <th>Employee Name</th>
            <td>@Model.EmployeeName</td>
        </tr>
        <tr>
            <th>Address</th>
            <td>@Model.Address</td>
        </tr>
        <tr>
            <th>Employee City</th>
            <td>@Model.City</td>
        </tr>
        <tr>
            <th>Country</th>
            <td>@Model.Country</td>
        </tr>
        <tr>
            <th>Zipcode</th>
            <td>@Model.Zipcode</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <th>Contact #</th>
            <td>@Model.Phone</td>
        </tr>
        <tr>
            <th>Employee Skillsets</th>
            <td>@Model.SkillSets</td>
        </tr>
        <tr>
            <th>Avatar</th>
            <td>
                <img src=@Url.Content(Model.Avatar) alt="@Model.EmployeeName" style=" height: 130px;" />
            </td>
        </tr>
    </tbody>
</table>
